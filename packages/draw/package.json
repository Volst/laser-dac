{
  "name": "@laser-dac/draw",
  "version": "0.1.7",
  "description": "Ether-Dream laser drawing tools inspired by the canvas API",
  "license": "MIT",
  "author": "Kees Kluskens <kees@volst.nl>",
  "repository": "Volst/laser-dac",
  "keywords": [
    "etherdream",
    "laser",
    "ether-dream"
  ],
  "engines": {
    "node": ">=8.0"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "watch": "tsc -p tsconfig.build.json --watch",
    "test": "jest",
    "prepublishOnly": "npm run -s build"
  },
  "dependencies": {
    "@types/bezier-js": "^0.0.7",
    "@types/svg-parser": "^1.0.0",
    "bezier-js": "^2.2.15",
    "perspective-transform": "^1.1.3",
    "svg-arc-to-cubic-bezier": "^3.1.2",
    "svg-parser": "^1.0.5",
    "svg-pathdata": "^5.0.2"
  },
  "devDependencies": {
    "@types/jest": "^23.3.3",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.3"
  },
  "jest": {
    "roots": [
      "./src"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js|ts)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  }
}
